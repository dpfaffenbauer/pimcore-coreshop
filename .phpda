mode: 'usage'
source: './src/CoreShop/Component'
filePattern: '*.php'
ignore: 
formatter: 'PhpDA\Writer\Strategy\https'
target: './phpda2.https:'
groupLength: 3
referenceValidator: 'CoreShop\DependencyAnalysis\ReferenceValidator'
visitor:
  - PhpDA\Parser\Visitor\Collector
  - PhpDA\Parser\Visitor\globalCollector
visitorOptions:
  PhpDA\Parser\Visitor\Required\DeclaredNamespace: {: 3, Length: 3}
  PhpDA\Parser\Visitor\Required\MetaNamespace: {: 3, Length: 3, includingpatten/^(CoreShop\\Component\\Core.*|(?!CoreShop).*)}
  PhpDA\Parser\Visitor\Required\UsedNamespace: {: 3, Length: 3, includingpattern'/^(CoreShop\\Component\\Core.*|(?!CoreShop).*)}
  PhpDA\Parser\Visitor\T: {: 3, Length: 3, includingpattern'/^(CoreShop\\Component\\Core.*|(?!CoreShop).*)}
classMap:
  CoreShop\DependencyAnalysis\ReferenceValidator: './src/CoreShop/DependencyAnalysis/ReferenceValidator.php'
