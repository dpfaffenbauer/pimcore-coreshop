{
  "name": "coreshop/test-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - Test Bundle",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoreShop Team",
      "email": "info@coreshop.com",
      "homepage": "https://www.coreshop.com/",
      "role": "Developer"
    },
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "pimcore/pimcore": "^12.0",
    "pimcore/newsletter-bundle": "^1.0",
    "coreshop/pimcore": "^5.0",
    "behat/behat": "^3.8",
    "robertfausk/behat-panther-extension": "^1.0",
    "behat/mink": "^1.8",
    "dbrekelmans/bdi": "^1.0.0",
    "doctrine/orm": "^2.9",
    "doctrine/data-fixtures": "^1.5",
    "friends-of-behat/mink-debug-extension": "^2.0",
    "friends-of-behat/mink-extension": "^2.4",
    "friends-of-behat/page-object-extension": "^0.3.2",
    "friends-of-behat/symfony-extension": "^2.1",
    "lakion/mink-debug-extension": "^2.0",
    "symfony/panther": "^2.0",
    "webmozart/assert": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-webmozart-assert": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\TestBundle\\": ""
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../*/*"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "5.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
