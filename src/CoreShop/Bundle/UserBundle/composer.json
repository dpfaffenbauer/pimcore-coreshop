{
  "name": "coreshop/user-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - User Bundle",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoreShop Team",
      "email": "info@coreshop.com",
      "homepage": "https://www.coreshop.com/",
      "role": "Developer"
    },
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/user": "^4.0",
    "coreshop/resource-bundle": "^4.0",
    "pimcore/pimcore": "^11.0 || ^12.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-webmozart-assert": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\UserBundle\\": ""
    }
  },
  "repositories": [
      {
          "type": "path",
          "url": "../../*/*"
      }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.1-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
